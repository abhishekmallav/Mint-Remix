alias neo="neofetch | lolcat"
alias python=python3
alias py=python3
alias py2=python2
alias cat=lolcat
alias ping="ping -c10 1.1.1.1 | lolcat"

alias update="sudo apt update"
alias upgrade="sudo apt upgrade"
alias install="sudo apt install"
alias search="sudo apt search"
alias purge="sudo apt purge"
alias remove="sudo apt remove"
alias autoremove="sudo apt autoremove"
alias autoclean="sudo apt autoclean"

alias hist="history | lolcat"
alias logout="cinnamon-session-quit --logout --force"

alias fp=flatpak
alias fpclean="flatpak uninstall --unused | lolcat"
alias fplist="flatpak --columns=name,size,app list"
alias fpsize="flatpak list --app --columns=size,name|sort -g"

alias wget="wget --directory-prefix=$HOME/Data"

alias uad="/home/natalie/Data/Apps/uad"
alias getty="~/Data/Apps/getty"
alias ventoy="sudo ~/Data/Apps/ventoy"
alias speedtest="speedtest-cli | lolcat"


